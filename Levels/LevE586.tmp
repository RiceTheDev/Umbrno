resource_name = "Level 1 Node Script"
script/source = "extends Node2D

@onready var speedrun_timer_2: Timer = $speedrunTimer2
@onready var current_timer: RichTextLabel = $UI/CanvasLayer/Panel/CurrentTimer
var can_start_timer = false
@onready var speedrun_timer: Timer = $speedrunTimer
@export var speedrunMode = false
@onready var player: CharacterBody2D = $CharacterBody2D
var current_time : float = 0
var current_time2 : float = 0

func new_timer():
	speedrun_timer.start(1)
func new_timer2():
	speedrun_timer_2.start(0.1)
func _ready() -> void:
	pass
	

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	print(current_time)
	if speedrunMode == true:
		if speedrun_timer.time_left <= 0.05:
			current_time += 1
			new_timer()
		if speedrun_timer_2.time_left <= 0.05:
			current_time2 += 0.1
			new_timer2()
		if !Input.is_anything_pressed():
			can_start_timer = true
	current_timer.text = str(\" \" + str(int(current_time)) + \".\" + str((current_time2)))
"

